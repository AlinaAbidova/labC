#include <stdio.h>

int main() {
 double matrix[3][3];
 double sum_main_diag = 0;
 double sum_secondary_diag = 0;

 // Заполнение массива данными с консоли
 printf("Введите значения массива 3x3:\n");
 for (int i = 0; i < 3; i++) {
 for (int j = 0; j < 3; j++) {
 scanf("%lf", &matrix[i][j]);
 }
 }

 // Вычисление суммы элементов на главной диагонали
 for (int i = 0; i < 3; i++) {
 sum_main_diag += matrix[i][i];
 }

 // Вычисление суммы элементов на побочной диагонали
 for (int i = 0; i < 3; i++) {
 sum_secondary_diag += matrix[i][2 - i];
 }

 // Вывод результатов
 printf("Сумма элементов на главной диагонали: %lf\n", sum_main_diag);
 printf("Сумма элементов на побочной диагонали: %lf\n", sum_secondary_diag);

 return 0;
}
